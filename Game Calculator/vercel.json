{
    "version": 2,
    "name": "game-calculator",
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}
